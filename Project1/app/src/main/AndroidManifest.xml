<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.project1">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Project1.NoActionBar">
        <activity android:name=".SaveList"></activity>
        <activity android:name=".MedianPrice" />
        <activity android:name=".SearchCapacity" />
        <activity android:name=".ChangePrice" />
        <activity android:name=".RemoveSauna" />
        <activity android:name=".ShowSauna" />
        <activity android:name=".AddSauna" />
        <activity android:name=".LoadSaunaList" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>